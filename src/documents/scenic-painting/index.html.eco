---
isPage: true
section: scenic-painting
---

<div class="row">
  <div class="large-12 columns">
    <ul class="small-block-grid-3">
      <% for project in @getCollection("scenicPainting").toJSON(): %>
      <li>
        <a href="<%= project.url %>">
          <img src="<%= @getThumbnail(@getPrimaryImage(project)) %>" alt="">
        </a>
      </li>
      <% end %>
    </ul>
  </div>
</div>
